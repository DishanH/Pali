    {
      "number": 10,
      "pali": "Paṭhamabhikkhādāyikāvimānavatthu",
      "english": "",
      "sinhala": "",
      "paliTitle": ""
    },
    {
      "number": 270,
      "pali": "‘‘Abhikkantena vaṇṇena, yā tvaṃ tiṭṭhasi devate; Obhāsentī disā sabbā, osadhī viya tārakā.",
      "english": "",
      "sinhala": "",
      "paliTitle": ""
    },
    {
      "number": 271,
      "pali": "‘‘Kena tetādiso vaṇṇo…pe…vaṇṇo ca te sabbadisā pabhāsatī’’ti.",
      "english": "",
      "sinhala": "",
      "paliTitle": ""
    },
    {
      "number": 273,
      "pali": "Sā devatā attamanā…pe… yassa kammassidaṃ phalaṃ.",
      "english": "",
      "sinhala": "",
      "paliTitle": ""
    },
    {
      "number": 274,
      "pali": "‘‘Ahaṃ manussesu manussabhūtā, purimāya jātiyā manussaloke.",
      "english": "",
      "sinhala": "",
      "paliTitle": ""
    },
    {
      "number": 275,
      "pali": "‘‘Addasaṃ virajaṃ buddhaṃ, vippasannamanāvilaṃ; Tassa adāsahaṃ bhikkhaṃ, pasannā sehi pāṇibhi.",
      "english": "",
      "sinhala": "",
      "paliTitle": ""
    },
    {
      "number": 276,
      "pali": "‘‘Tena metādiso vaṇṇo…pe… Vaṇṇo ca me sabbadisā pabhāsatī’’ti. Paṭhamabhikkhādāyikāvimānaṃ dasamaṃ.",
      "english": "",
      "sinhala": "",
      "paliTitle": ""
    }
  ],
