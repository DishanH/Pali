    {
      "number": 11,
      "pali": "Khīrodanadāyikāvimānavatthu",
      "english": "",
      "sinhala": "",
      "paliTitle": ""
    },
    {
      "number": 409,
      "pali": "‘‘Abhikkantena vaṇṇena…pe… osadhī viya tārakā.",
      "english": "",
      "sinhala": "",
      "paliTitle": ""
    },
    {
      "number": 410,
      "pali": "‘‘Kena tetādiso vaṇṇo…pe…vaṇṇo ca te sabbadisā pabhāsatī’’ti.",
      "english": "",
      "sinhala": "",
      "paliTitle": ""
    },
    {
      "number": 412,
      "pali": "Sā devatā attamanā…pe… yassa kammassidaṃ phalaṃ.",
      "english": "",
      "sinhala": "",
      "paliTitle": ""
    },
    {
      "number": 413,
      "pali": "‘‘Khīrodanaṃ ahamadāsiṃ, bhikkhuno piṇḍāya carantassa; Evaṃ karitvā kammaṃ, sugatiṃ upapajja modāmi.",
      "english": "",
      "sinhala": "",
      "paliTitle": ""
    },
    {
      "number": 414,
      "pali": "‘‘Tassā me passa vimānaṃ, accharā kāmavaṇṇinīhamasmi; Accharāsahassassāhaṃ, pavarā passa puññānaṃ vipākaṃ.",
      "english": "",
      "sinhala": "",
      "paliTitle": ""
    },
    {
      "number": 415,
      "pali": "‘‘Tena metādiso vaṇṇo…pe…vaṇṇo ca me sabbadisā pabhāsatī’’ti. (Anantaraṃ pañcavīsativimānaṃ yathā khīrodanadāyikāvimānaṃ tathā vitthāretabbaṃ) [( ) natthi sī. potthake]",
      "english": "",
      "sinhala": "",
      "paliTitle": ""
    },
