    {
      "number": 755,
      "pali": "‘‘Abhikkantena vaṇṇena…pe…vaṇṇo ca te sabbadisā pabhāsatī’’ti.",
      "english": "",
      "sinhala": "",
      "paliTitle": ""
    },
    {
      "number": 758,
      "pali": "Sā devatā attamanā…pe… yassa kammassidaṃ phalaṃ.",
      "english": "",
      "sinhala": "",
      "paliTitle": ""
    },
    {
      "number": 759,
      "pali": "‘‘Indīvarānaṃ hatthakaṃ ahamadāsiṃ, bhikkhuno piṇḍāya carantassa; Esikānaṃ uṇṇatasmiṃ, nagaravare paṇṇakate ramme.",
      "english": "",
      "sinhala": "",
      "paliTitle": ""
    },
    {
      "number": 760,
      "pali": "‘‘Tena metādiso vaṇṇo…pe…vaṇṇo ca me sabbadissā pabhāsatī’’ti.",
      "english": "",
