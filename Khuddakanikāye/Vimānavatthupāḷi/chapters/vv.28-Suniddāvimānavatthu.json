{
  "id": "vv.28",
  "title": {
    "pali": "Suniddāvimānavatthu",
    "english": "",
    "sinhala": ""
  },
  "sections": [
    {
      "number": 258,
      "pali": "‘‘Abhikkantena vaṇṇena…pe… osadhī viya tārakā.",
      "english": "",
      "sinhala": "",
      "paliTitle": ""
    },
    {
      "number": 259,
      "pali": "‘‘Kena tetādiso vaṇṇo…pe… vaṇṇo ca te sabbadisā pabhāsatī’’ti.",
      "english": "",
      "sinhala": "",
      "paliTitle": ""
    },
    {
      "number": 261,
      "pali": "Sā devatā attamanā…pe… yassa kammassidaṃ phalaṃ.",
      "english": "",
      "sinhala": "",
      "paliTitle": ""
    },
    {
      "number": 262,
      "pali": "‘‘Suniddāti [sunandāti (sī.)] maṃ aññaṃsu, rājagahasmiṃ upāsikā; Saddhā sīlena sampannā, saṃvibhāgaratā sadā.",
      "english": "",
      "sinhala": "",
      "paliTitle": ""
    },
    {
      "number": 266,
      "pali": "(Yathā niddāvimānaṃ tathā vitthāretabbaṃ.)",
      "english": "",
      "sinhala": "",
      "paliTitle": ""
    },
    {
      "number": 267,
      "pali": "‘‘Pañcasikkhāpade ratā, ariyasaccāna kovidā; Upāsikā cakkhumato, gotamassa yasassino.",
      "english": "",
      "sinhala": "",
      "paliTitle": ""
    },
    {
      "number": 268,
      "pali": "‘‘Tena metādiso vaṇṇo…pe… vaṇṇo ca me sabbadisā pabhāsatī’’ti. Suniddāvimānaṃ navamaṃ.",
      "english": "",
      "sinhala": "",
      "paliTitle": ""
    }
  ],
